	.popup_form_p
		.popup_close.close_popups.remove 
			button(type="button")
				+icon('close') 
		.popup_form_content_w
			.popup_form_content
				.popup_form_left 
					.popup_slider_nav 
							.popup_slider_btn_prewios 
								button(type="button")
									+icon('previous')
